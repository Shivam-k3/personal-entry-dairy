{
  "version": 2,
  "builds": [
    {
      "src": "api/register.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/login.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/register",
      "dest": "/api/register.py"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/login.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
} 